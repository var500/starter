{
	"arrowParens": "always",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"jsxSingleQuote": false,
	"endOfLine": "auto",
	"printWidth": 100,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"useTabs": true,
	"tabWidth": 4,
	"overrides": [
		{
			"files": [".prettierrc", ".eslintrc", ".lintstagedrc"],
			"options": {
				"parser": "json"
			}
		}
	]
}
